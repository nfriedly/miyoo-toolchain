prefix=/opt/miyoo
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

gmodule_supported=true

Name: GModule
Description: Dynamic module loader for GLib
Requires: glib-2.0
Version: 2.54.2
Libs: -L${libdir} -Wl,--export-dynamic -lgmodule-2.0 -pthread
Libs.private: -ldl
Cflags: -pthread
